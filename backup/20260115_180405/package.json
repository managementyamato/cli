{
  "name": "trouble-management",
  "version": "1.0.0",
  "description": "YA管理一覧システム",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run watch\"",
    "server": "php -S localhost:8000",
    "watch": "browser-sync start --proxy 'localhost:8000' --files '**/*.php, **/*.css, **/*.js' --no-notify --reload-delay 300 --no-open",
    "preview": "npm run dev"
  },
  "devDependencies": {
    "browser-sync": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "author": "",
  "license": "ISC"
}
